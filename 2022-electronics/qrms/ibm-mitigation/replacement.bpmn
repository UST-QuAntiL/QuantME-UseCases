function generateArray(length){
	var array = []
	var iterator = new java.util.Iterator({
	  i: 0,
	  hasNext: function() {
		return this.i &lt; length;
	  },
	  next: function() {
		return this.i++;
	  }
	});

	print(iterator instanceof Java.type("java.util.Iterator"));
	while (iterator.hasNext()) {
	  value = iterator.next();
	  array.push(value);
	}
	print(array)
	return array
}	

